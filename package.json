{
  "name": "instagram-save",
  "version": "1.3.1",
  "description": "Downloads and saves Instagram photos and videos to your current working directory.",
  "main": "index.js",
  "engine-strict": true,
  "engines": {
    "node": ">=4.0.0"
  },
  "bin": {
    "instagram-save": "bin/cli.js"
  },
  "preferGlobal": "true",
  "scripts": {
    "test": "node node_modules/mocha/bin/mocha test/**/*-spec.js --timeout 5000",
    "lint": "node node_modules/eslint/bin/eslint.js index.js lib bin test",
    "prepublish": "npm test && npm run lint"
  },
  "keywords": [
    "instagram",
    "downloader",
    "saver",
    "save",
    "download",
    "export",
    "cli",
    "command-line",
    "image",
    "photo",
    "picture",
    "video",
    "media"
  ],
  "author": "Eric Nishio <eric@self-learner.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericnishio/instagram-save.git"
  },
  "dependencies": {
    "async": "^2.6.0",
    "cheerio": "^0.22.0",
    "cli-color": "^1.1.0",
    "lodash": "^4.17.10",
    "request": "^2.86.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "sinon": "^5.0.7"
  }
}
